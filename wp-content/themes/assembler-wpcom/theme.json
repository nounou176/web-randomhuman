{
    "customTemplates": [
        {
            "name": "order-confirmation",
            "title": "Order Confirmation"
        },
        {
            "name": "page-cart",
            "title": "Page: Cart"
        },
        {
            "name": "page-checkout",
            "title": "Page: Checkout"
        },
        {
            "name": "template-page-w-hero",
            "title": "Page with Hero"
        },
        {
            "name": "template-page-w-title",
            "title": "Page with Title"
        },
        {
            "name": "archive-product",
            "title": "Product Catalogue"
        },
        {
            "name": "product-search-results",
            "title": "Product Search Results"
        },
        {
            "name": "taxonomy-product_attribute",
            "title": "Products by Attribute"
        },
        {
            "name": "single-product",
            "title": "Single Product"
        }
    ],
    "settings": {
        "appearanceTools": true,
        "color": {
            "defaultDuotone": false,
            "defaultGradients": false,
            "defaultPalette": false,
            "palette": [
                {
                    "color": "#FFFFFF",
                    "name": "Color 1",
                    "slug": "theme-1"
                },
                {
                    "color": "#EEEEEE",
                    "name": "Color 2",
                    "slug": "theme-2"
                },
                {
                    "color": "#BBBBBB",
                    "name": "Color 3",
                    "slug": "theme-3"
                },
                {
                    "color": "#1E1E1E",
                    "name": "Color 4",
                    "slug": "theme-4"
                },
                {
                    "color": "#000000",
                    "name": "Color 5",
                    "slug": "theme-5"
                }
            ],
            "duotone": [
                {
                    "colors": [
                        "#000000",
                        "#FFFFFF"
                    ],
                    "slug": "duotone-1",
                    "name": "Duotone 1"
                },
                {
                    "colors": [
                        "#000000",
                        "#BBBBBB"
                    ],
                    "slug": "duotone-2",
                    "name": "Duotone 2"
                }
            ]
        },
        "custom": {
            "spacing-increment": "2.5",
            "spacing-unit": "10"
        },
        "layout": {
            "contentSize": "620px",
            "wideSize": "1440px"
        },
        "blocks": {
            "core/heading": {
                "color": {
                    "background": false
                }
            }
        },
        "spacing": {
            "defaultSpacingSizes": false,
            "spacingScale": {
                "steps": 1
            },
            "spacingSizes": [
                {
                    "name": "2X-Small",
                    "size": "min(calc(var(--wp--custom--spacing-unit) * (var(--wp--custom--spacing-increment) * 1) * 1px), calc(var(--wp--custom--spacing-increment) * 1 * 2vw))",
                    "slug": "20"
                },
                {
                    "name": "X-Small",
                    "size": "min(calc(var(--wp--custom--spacing-unit) * (var(--wp--custom--spacing-increment) * 2) * 1px), calc(var(--wp--custom--spacing-increment) * 2 * 1vw))",
                    "slug": "30"
                },
                {
                    "name": "Small",
                    "size": "min(calc(var(--wp--custom--spacing-unit) * (var(--wp--custom--spacing-increment) * 3) * 1px), calc(var(--wp--custom--spacing-increment) * 3 * 1vw))",
                    "slug": "40"
                },
                {
                    "name": "Medium",
                    "size": "min(calc(var(--wp--custom--spacing-unit) * (var(--wp--custom--spacing-increment) * 4) * 1px), calc(var(--wp--custom--spacing-increment) * 4 * 1vw))",
                    "slug": "50"
                },
                {
                    "name": "Large",
                    "size": "min(calc(var(--wp--custom--spacing-unit) * (var(--wp--custom--spacing-increment) * 5) * 1px), calc(var(--wp--custom--spacing-increment) * 5 * 1vw))",
                    "slug": "60"
                },
                {
                    "name": "Extra Large",
                    "size": "min(calc(var(--wp--custom--spacing-unit) * (var(--wp--custom--spacing-increment) * 6) * 1px), calc(var(--wp--custom--spacing-increment) * 6 * 1vw))",
                    "slug": "70"
                },
                {
                    "name": "2X Large",
                    "size": "min(calc(var(--wp--custom--spacing-unit) * (var(--wp--custom--spacing-increment) * 7) * 1px), calc(var(--wp--custom--spacing-increment) * 7 * 1vw))",
                    "slug": "80"
                }
            ],
            "units": [
				"%",
				"px",
				"em",
				"rem",
				"vh",
				"vw"
			]
        },
        "typography": {
            "defaultFontSizes": false,
            "fluid": true,
            "fontFamilies": [
                {
                    "fontFace": [
                        {
                            "fontFamily": "Inter",
                            "fontStretch": "normal",
                            "fontStyle": "normal",
                            "fontWeight": "100 800",
                            "src": [
                                "file:./assets/fonts/inter/InterVariable.woff2"
                            ]
                        },
                        {
                            "fontFamily": "Inter",
                            "fontStretch": "normal",
                            "fontStyle": "italic",
                            "fontWeight": "100 800",
                            "src": [
                                "file:./assets/fonts/inter/InterVariable-Italic.woff2"
                            ]
                        }
                    ],
                    "fontFamily": "\"Inter\", sans-serif",
                    "name": "Inter",
                    "slug": "inter"
                }
            ],
            "fontSizes": [
                {
                    "name": "Small",
                    "size": "16px",
                    "slug": "small"
                },
                {
                    "name": "Medium",
                    "size": "24px",
                    "slug": "medium",
                    "fluid": {
                        "max": "20px",
                        "min": "24px"
                    }
                },
                {
                    "name": "Large",
                    "size": "38px",
                    "slug": "large"
                },
                {
                    "name": "Extra Large",
                    "size": "60px",
                    "slug": "x-large"
                },
                {
                    "fluid": {
                        "max": "80px",
                        "min": "40px"
                    },
                    "name": "2X Large",
                    "size": "80px",
                    "slug": "xx-large"
                },
                {
                    "name": "3X Large",
                    "size": "160px",
                    "slug": "xxx-large",
                    "fluid": {
                        "min": "40px",
                        "max": "160px"
                    }
                }
            ],
            "writingMode": true
        },
        "useRootPaddingAwareAlignments": true
    },
    "styles": {
        "blocks": {
            "core/button": {
                "variations": {
                    "outline": {
                        "border": {
                            "width": "1px"
                        },
                        "spacing": {
                            "padding": {
                                "bottom": "15px",
                                "left": "23px",
                                "right": "23px",
                                "top": "15px"
                            }
                        }
                    }
                }
            },
            "core/buttons": {
                "spacing": {
                    "blockGap": "8px",
                    "margin": {
                        "top": "calc( var(--wp--style--block-gap) * 1.5)"
                    }
                }
            },
            "core/columns": {
                "spacing": {
                    "blockGap": {
                        "left": "var(--wp--preset--spacing--40)",
                        "top": "var(--wp--preset--spacing--30)"
                    }
                }
            },
            "core/navigation": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--small)",
                    "lineHeight": "1.3"
                }
            },
            "core/post-content": {
                "css": "& > div:first-child.alignfull { margin-top: calc( -1 * var(--wp--preset--spacing--20)) !important; } & > div:last-child.alignfull { margin-bottom: calc( -1 * var(--wp--preset--spacing--60)) !important; } & > p + div.alignfull { margin-top: var(--wp--style--block-gap) !important; }",
                "spacing": {
                    "padding": {
                        "top": "var(--wp--preset--spacing--20)",
                        "bottom": "var(--wp--preset--spacing--60)"
                    }
                }
            },
            "core/separator": {
                "border": {
                    "top": {
                        "width": "2px"
                    }
                },
                "color": {
                    "text": "currentColor"
                }
            },
            "core/site-title": {
                "elements": {
                    "link": {
                        "typography": {
                            "textDecoration": "none"
                        }
                    }
                },
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--medium)",
                    "fontWeight": "550",
                    "lineHeight": {
                        "ref": "styles.blocks.core/navigation.typography.lineHeight"
                    }
                }
            },
            "woocommerce/accordion-header": {
                "typography": {
                    "fontSize":  "var(--wp--preset--font-size--small)"
                }
            },
            "woocommerce/accordion-group": {
                "spacing": {
                    "blockGap": "0"
                }
            },
            "woocommerce/add-to-cart-with-options-variation-selector-attribute-name" : {
                "typography": {
                    "fontSize":  "var(--wp--preset--font-size--small)"
                }
            },
            "woocommerce/order-confirmation-status": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--medium)"
                }
            },
            "woocommerce/product-price": {
                "typography": {
                    "fontSize":  "var(--wp--preset--font-size--medium)",
                    "lineHeight": "1.2"
                }
            },
            "woocommerce/product-title": {
                "typography": {
                    "fontSize":  "var(--wp--preset--font-size--medium)",
                    "lineHeight": "1.2"
                }
            }
        },
        "color": {
            "background": "var(--wp--preset--color--theme-1)",
            "text": "var(--wp--preset--color--theme-4)"
        },
        "css": ".no-underline a { text-decoration: none; } button:focus, .wp-block-button__link:focus { outline: 2px solid var(--wp--preset--color--theme-4); outline-offset: 2px; } button[disabled], .wp-block-button__link[disabled], .wp-block-button__link:disabled { opacity: 0.5; cursor: not-allowed; } button[disabled]:hover, .wp-block-button__link[disabled]:hover, .wp-block-button__link:disabled:hover { background-color: var(--wp--preset--color--theme-4); }",
        "elements": {
            "button": {
                ":hover": {
                    "color": {
                        "background": "color-mix(in srgb, var(--wp--preset--color--theme-5) 85%, #000)"
                    }
                },
                "border": {
                    "radius": "0"
                },
                "color": {
                    "background": "var(--wp--preset--color--theme-4)",
                    "text": "var(--wp--preset--color--theme-1)"
                },
                "spacing": {
                    "padding": {
                        "bottom": "16px",
                        "left": "24px",
                        "right": "24px",
                        "top": "16px"
                    }
                },
                "typography": {
                    "fontFamily": {
                        "ref": "styles.elements.heading.typography.fontFamily"
                    },
                    "fontSize": "var(--wp--preset--font-size--small)",
                    "fontWeight": "450"
                }
            },
            "h1": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--xx-large)",
                    "lineHeight": "1"
                }
            },
            "h2": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--x-large)",
                    "fontWeight": {
                        "ref": "styles.elements.h1.typography.fontWeight"
                    },
                    "lineHeight": "1"
                }
            },
            "h3": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--medium)",
                    "lineHeight": "1.2"
                }
            },
            "h4": {
                "typography": {
                    "fontSize": "clamp(22px, 1.625rem + ((1vw - 3px) * -0.536), 18px)",
                    "lineHeight": "1.3"
                }
            },
            "h5": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--medium)",
                    "lineHeight": "1.4"
                }
            },
            "h6": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--small)"
                }
            },
            "heading": {
                "css": "&.has-background { padding: 0; }",
                "typography": {
                    "fontWeight": "500"
                }
            },
            "caption": {
                "spacing": {
                    "padding": {
                        "top": "8px"
                    }
                },
                "typography": {
                    "fontWeight": "400",
                    "fontSize": "14px"
                }
            },
            "link": {
                "color": {
                    "text": "currentColor"
                }
            }
        },
        "spacing": {
            "blockGap": "var(--wp--preset--spacing--20)",
            "padding": {
                "left": "var(--wp--preset--spacing--40)",
                "right": "var(--wp--preset--spacing--40)"
            }
        },
        "typography": {
            "fontFamily": "var(--wp--preset--font-family--inter)",
            "fontSize": "16px",
            "fontWeight": "400",
            "lineHeight": "1.65"
        }
    },
